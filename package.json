{
  "name": "react-mosaic-component",
  "version": "6.1.1",
  "description": "A React Tiling Window Manager",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "style": "lib/react-mosaic.css",
  "type": "commonjs",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/nomcopter/react-mosaic.git"
  },
  "funding": "https://github.com/nomcopter/react-mosaic?sponsor=1",
  "keywords": [
    "ui",
    "react",
    "component",
    "typescript",
    "tiling-window-manager",
    "window-manager"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "npm-run-all clean -lp build:**",
    "build:ts": "tsc -p tsconfig-build.json",
    "build:less": "lessc --autoprefix=defaults styles/index.less react-mosaic-component.css",
    "bundle": "rm -rf docs/ && webpack --config webpack/bundle.ts",
    "clean": "rm -rf lib/",
    "start": "webpack-dev-server --config webpack/hot.ts",
    "prettier:run": "prettier 'styles/*.less' '*.md' '{,.}*.yml' '.circleci/*.yml'",
    "test": "npm-run-all build -lp test:**",
    "test:lint": "tslint -c tslint.yml -p tsconfig.json -e test",
    "test:unit": "mocha",
    "test:unit:watch": "mocha --watch",
    "test:components": "mocha --spec 'test/componentSpec.tsx'",
    "test:integration": "mocha --spec 'test/integrationSpec.tsx'",
    "test:format": "yarn run prettier:run --list-different",
    "fix": "npm-run-all -lp fix:**",
    "fix:format": "yarn run prettier:run --write",
    "fix:lint": "yarn run test:lint --fix",
    "version": "npm-run-all test bundle && git add -A docs/"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "rdndmb-html5-to-touch": "^9.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-multi-backend": "^9.0.0",
    "react-dnd-touch-backend": "^16.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@blueprintjs/core": "^6.8.0",
    "@blueprintjs/icons": "^6.5.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/chai": "^4.3.20",
    "@types/classnames": "^2.3.4",
    "@types/dom4": "^2.0.4",
    "@types/lodash": "^4.17.23",
    "@types/mocha": "^10.0.10",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/sinon": "^21.0.0",
    "@types/uuid": "^9.0.8",
    "@types/webpack": "^5.28.5",
    "chai": "^4.5.0",
    "css-loader": "^7.1.4",
    "dnd-core": "16.0.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.6",
    "jsdom": "^25.0.1",
    "jsdom-global": "^3.0.2",
    "less": "^4.2.1",
    "less-loader": "^12.3.1",
    "less-plugin-autoprefix": "^2.0.0",
    "mocha": "^10.8.2",
    "mocha-junit-reporter": "^2.2.1",
    "mock-require": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-refresh": "^0.14.2",
    "sinon": "^21.0.1",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "webpack": "^5.105.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.3",
    "yarn-deduplicate": "^6.0.2"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}
